# Run this Python snippet once to clean the JSON:
import json
with open("memory_vector_v12.json") as f:
    data = json.load(f)
# Recursively strip spaces from keys/values
cleaned = {k.strip(): {kk.strip(): vv.strip() if isinstance(vv, str) else vv 
                       for kk, vv in v.items()} 
           for k, v in data["concepts "].items()}
with open("memory_vector_v12_clean.json", "w") as f:
    json.dump({"concepts": cleaned}, f, indent=2)